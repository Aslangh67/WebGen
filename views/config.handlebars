                    <div class="bar"></div>
               </div> <!--Hamburger End-->
               
               <ul class="nav-group hidden"> <!--Navgroup Start-->
                    
              

               <li class="nav-link"><a href="/{{dataValues.route}}/menu" class="nav-link-text menu-link">Menu</a>
               </li>
               <li class="nav-link"><a href="/{{dataValues.route}}" class="nav-link-text log-off-link">Log Off</a>
               </li>

    



     <div class="hidden-field hidden" id="route">
     {{account.route}}
</div>
<div class="hidden-field1 hidden" id="accountId">
     {{account.id}}
</div>
<div class="hidden-field2 hidden" id="contentId">
     {{webContent.id}}
</div>
     
     
     <div id="web-content" class="customize-overlay hidden">
          <div class="close-overlay">
               <i class="fas fa-times"></i>
          </div>

          <div id="add-description-modal" class="hidden">
               <div class="modalStructure">
                    <h4>Update Description</h4>
                    <form>
                         <textarea id="web-desc"> {{webContent.description}}</textarea>
                    
                         <input type="submit" value="Submit" class="submit-descript-btn" name="action">
                    </form>
               </div>
          </div>
     </div>

     <div id="manage-employee-modal" class="hidden">
          <div class="modalStructure modalStructure2">
               
               <i class="fas fa-times close-manage-employees-btn"></i>  

               <div class="modalStructure-header">
                    <h4>Manage Employees</h4>
               </div>

               <div class="manage-emp-body">
                    <ul class="employee-list">
                         <li class="an-employee">
                              <div class="employee-info">
                                   <div class="emp-name-field">
                                        Name: <span class="emp-name"></span>
                                   </div>
                                   <div class="emp-email-field">
                                        Email: <span class="emp-email"></span>
                                   </div>
                                   <div class="emp-password-field">
                                        Role: <span class="emp-password"></span>
                                   </div>
                              </div>
                              <div class="employee-btn-group">
                                   <button id="update-employee-info">
                                        <i class="fas fa-edit"></i>
                                   </button>
                                   
                                   <button id="delete-employee">
                                        <i class="fas fa-trash-alt"></i>
                                   </button>
                              </div>
                         </li>
                    </ul>

                    <i class="fas fa-plus-circle add-employee-btn"></i>
               </div>

               

                <div id="add-employee-form" class="hidden">
                    <form>
                         <div class="form-inner">
                              <input type="text" id="new-user-name" placeholder="Name*">
                                        
                              <input type="text" id="new-user-email" placeholder="Email*">
                              
                              <input type="password" id="new-user-password" placeholder="Enter Password*">

                              <div class="select-role" id="new-user-role">
                                   <p>Please select Employee's Role:</p>
                                        <input id="web-designer" type="radio" name="role" value=2>
                                        <label for="web-designer">Web Designer</label><br>
                                        
                                        <input id="chef" type="radio" name="role" value=3>
                                        <label for="chef">Chef</label><br>
     
                                        <input id="hostess" type="radio" name="role" value=4
                                        <label for="hostess">Host/Hostess</label><br>
                              </div>
                         </div>
                         
                         
                         

                         <input type="button" value="Submit" id="add-user-btn" name="action">
                    </form>
               </div>
          </div>
     </div>
     

